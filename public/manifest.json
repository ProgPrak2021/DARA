{
  "name": "DARA",
  "description": "Data Automated Request Analysis",
  "manifest_version": 3,
  "version": "1.0.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab", "tabs"],
  "host_permissions": [
    "https://www.instagram.com/*",
    "https://www.facebook.com/*"
  ]
}
